<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ClientWebServicePortBinding" />
                <binding name="ProductWebServicePortBinding" />
                <binding name="ProductRegistrationWebServicePortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://192.168.16.14:7101/ServerBankSW/ProductRegistrationWebServicePort"
                binding="basicHttpBinding" bindingConfiguration="ProductRegistrationWebServicePortBinding"
                contract="ProductRegistrationWebService.ProductRegistrationWebService"
                name="ProductRegistrationWebServicePort" />
            <endpoint address="http://192.168.16.14:7101/ServerBankSW/ProductWebServicePort"
                binding="basicHttpBinding" bindingConfiguration="ProductWebServicePortBinding"
                contract="ProductWebService.ProductWebService" name="ProductWebServicePort" />
            <endpoint address="http://192.168.16.14:7101/ServerBankSW/ClientWebServiceSW"
                binding="basicHttpBinding" bindingConfiguration="ClientWebServicePortBinding"
                contract="ClientWebService.ClientWebService" name="ClientWebServicePort" />
        </client>
    </system.serviceModel>
</configuration>